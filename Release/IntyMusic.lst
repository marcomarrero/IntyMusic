BAEC   000C                     	INCR R4
BAED   02FD 0002                	ADDI #2,R5	; Avoid $01F6 and $01F7
BAEF   0201 0006                	BC @@4		; Jump if playing with drums
BAF1   02FC 0002                	ADDI #2,R4
BAF3   02FD 0003                	ADDI #3,R5
BAF5   0200 0005                	B @@5

BAF7   02A0                     @@4:	MVI@ R4,R0
BAF8   0268                     	MVO@ R0,R5	; $01F8
BAF9   02A0                             MVI@ R4,R0
BAFA   0268                     	MVO@ R0,R5	; $01F9
BAFB   000D                     	INCR R5		; Avoid $01FA
