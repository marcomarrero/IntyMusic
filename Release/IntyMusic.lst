ACB3   007A                     	SARC R2,1
ACB4   0204 0044                	BEQ @@6
ACB6   02BC 0124                	MVII #_music_freq10,R4
ACB8   02BD 01F0                	MVII #$01F0,R5
ACBA   02A0                             MVI@ R4,R0
ACBB   0268                     	MVO@ R0,R5	; $01F0 - Channel A Period (Low 8 bits of 12)
ACBC   02A0                             MVI@ R4,R0
ACBD   0268                     	MVO@ R0,R5	; $01F1 - Channel B Period (Low 8 bits of 12)
ACBE   0012                     	DECR R2
ACBF   0204 001E                	BEQ @@1
ACC1   02A0                             MVI@ R4,R0	
ACC2   0268                     	MVO@ R0,R5	; $01F2 - Channel C Period (Low 8 bits of 12)
ACC3   000D                     	INCR R5		; Avoid $01F3 - Enveloped Period (Low 8 bits of 16)
ACC4   02A0                             MVI@ R4,R0
ACC5   0268                     	MVO@ R0,R5	; $01F4 - Channel A Period (High 4 bits of 12)
ACC6   02A0                             MVI@ R4,R0
ACC7   0268                     	MVO@ R0,R5	; $01F5 - Channel B Period (High 4 bits of 12)
ACC8   02A0                             MVI@ R4,R0
ACC9   0268                     	MVO@ R0,R5	; $01F6 - Channel C Period (High 4 bits of 12)
ACCA   000D                     	INCR R5		; Avoid $01F7 - Envelope Period (High 8 bits of 16)
ACCB   0201 0006                	BC @@2		; Jump if playing with drums
ACCD   02FC 0002                	ADDI #2,R4
ACCF   02FD 0003                	ADDI #3,R5
ACD1   0200 0005                	B @@3

ACD3   02A0                     @@2:	MVI@ R4,R0
ACD4   0268                     	MVO@ R0,R5	; $01F8 - Enable Noise/Tone (bits 3-5 Noise : 0-2 Tone)
ACD5   02A0                             MVI@ R4,R0	
ACD6   0268                     	MVO@ R0,R5	; $01F9 - Noise Period (5 bits)
ACD7   000D                     	INCR R5		; Avoid $01FA - Envelope Type (4 bits)
ACD8   02A0                     @@3:    MVI@ R4,R0
ACD9   0268                     	MVO@ R0,R5	; $01FB - Channel A Volume
ACDA   02A0                             MVI@ R4,R0
ACDB   0268                     	MVO@ R0,R5	; $01FC - Channel B Volume
ACDC   02A0                             MVI@ R4,R0
ACDD   0268                     	MVO@ R0,R5	; $01FD - Channel C Volume
ACDE   008F                             JR R1

ACDF   000C                     @@1:	INCR R4		
ACE0   02FD 0002                	ADDI #2,R5	; Avoid $01F2 and $01F3
ACE2   02A0                             MVI@ R4,R0
ACE3   0268                     	MVO@ R0,R5	; $01F4
ACE4   02A0                             MVI@ R4,R0
ACE5   0268                     	MVO@ R0,R5	; $01F5
ACE6   000C                     	INCR R4
ACE7   02FD 0002                	ADDI #2,R5	; Avoid $01F6 and $01F7
ACE9   0201 0006                	BC @@4		; Jump if playing with drums
ACEB   02FC 0002                	ADDI #2,R4
ACED   02FD 0003                	ADDI #3,R5
ACEF   0200 0005                	B @@5

ACF1   02A0                     @@4:	MVI@ R4,R0
ACF2   0268                     	MVO@ R0,R5	; $01F8
ACF3   02A0                             MVI@ R4,R0
ACF4   0268                     	MVO@ R0,R5	; $01F9
ACF5   000D                     	INCR R5		; Avoid $01FA
ACF6   02A0                     @@5:    MVI@ R4,R0
ACF7   0268                     	MVO@ R0,R5	; $01FB
ACF8   02A0                             MVI@ R4,R0
ACF9   0268                     	MVO@ R0,R5	; $01FD
ACFA   008F                     @@6:    JR R1
                                	ENDP

